{
  "name": "express-controller-pattern",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node ./src",
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "babel ./src -d dist",
    "build": "npm run clean && npm run build-babel",
    "serve": "npm run build && node ./dist/index.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~4.1.0",
    "express": "~4.16.4",
    "http-errors": "~1.7.1",
    "morgan": "~1.9.1",
    "require-directory": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "nodemon": "^1.18.8"
  }
}
